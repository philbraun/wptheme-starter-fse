{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "page-fullwidth-notitle",
			"title": "Page: Fullwidth / No title",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-fullwidth",
			"title": "Page: Fullwidth",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-notitle",
			"title": "Page: No title",
			"postTypes": [
				"page"
			]
		}
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "query",
			"title": "Query (The Loop)",
			"area": "query"
		}
	],
	"settings": {
		"color": {
			"appearanceTools": true,
			"defaultPalette": false,
			"custom": false,
			"duotone": [],
			"customDuotone": false,
			"defaultGradients": false,
			"customGradients": false,
			"link": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#5D4B54",
					"name": "Main Pink"
				},
				{
					"slug": "secondary",
					"color": "#4C3843",
					"name": "Dark Pink"
				},
				{
					"slug": "tertiary",
					"color": "#E3EAD2",
					"name": "Green"
				},
				{
					"slug": "quaternary",
					"color": "#EEE7CF",
					"name": "Yellow"
				},
				{
					"slug": "quinary",
					"color": "#EFEADF",
					"name": "Orange"
				},
				{
					"slug": "senary",
					"color": "#EBE1DB",
					"name": "Light Pink"
				},
				
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				}
			],
			"gradients": [
				{
					"slug": "quinary-to-quaternary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--quinary), var(--wp--preset--color--quaternary))",
					"name": "Orange to Yellow"
				},
				{
					"slug": "quaternary-to-quinary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--quaternary), var(--wp--preset--color--quinary))",
					"name": "Yellow to Orange"
				},
				{
					"slug": "tertiary-to-quaternary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--tertiary), var(--wp--preset--color--quaternary))",
					"name": "Green to Yellow"
				},
				{
					"slug": "quaternary-to-tertiary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--quaternary), var(--wp--preset--color--tertiary))",
					"name": "Yellow to Green"
				},
				{
					"slug": "tertiary-to-quinary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--tertiary), var(--wp--preset--color--quinary))",
					"name": "Green to Orange"
				},
				{
					"slug": "quinary-to-tertiary",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--quinary), var(--wp--preset--color--tertiary))",
					"name": "Orange to Green"
				}
			]
		},
		"custom": {
			"typography": {
				"line-height": {
					"body": 1.7,
					"heading": 1.3,
					"page-title": 1.1
				},
				"font-weight":{
					"light": "300",
					"normal": "400",
					"bold": "700",
					"black": "900"
				}
			},
			"spacing": {
				"unit": "20px",
				"horizontal": "25px",
				"vertical": "30px"
			}
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"lineHeight": true,
			"customFontSize": false,
			"fontFamilies": [
				{
					"fontFamily": "Lato,sansserif",
					"name": "Lato",
					"slug": "lato",
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/lato-v23-latin-regular.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [ "file:./assets/fonts/lato-v23-latin-italic.woff2" ]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/lato-v23-latin-700.woff2" ]
						}
					]
				},
				{
					"fontFamily": "Merriweather,serif",
					"name": "Merriweather",
					"slug": "merriweather",
					"fontFace": [
						{
							"fontFamily": "Merriweather",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/merriweather-v30-latin-regular.woff2" ]
						},
						{
							"fontFamily": "Merriweather",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [ "file:./assets/fonts/merriweather-v30-latin-italic.woff2" ]
						},
						{
							"fontFamily": "Merriweather",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/merriweather-v30-latin-700.woff2" ]
						}
					]
				},
				{
					"fontFamily": "Roboto,sansserif",
					"name": "Roboto",
					"slug": "roboto",
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/roboto-v30-latin-regular.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [ "file:./assets/fonts/roboto-v30-latin-italic.woff2" ]
						},
						{
							"fontFamily": "Roboto",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/roboto-v30-latin-700.woff2" ]
						}
					]
				},
				{
					"fontFamily": "\"Roboto Slab\",serif",
					"name": "Roboto Slab",
					"slug": "roboto-slab",
					"fontFace": [
						{
							"fontFamily": "\"Roboto Slab\"",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/roboto-slab-v23-latin-regular.woff2" ]
						},
						{
							"fontFamily": "\"Roboto Slab\"",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [ "file:./assets/fonts/roboto-slab-v23-latin-700.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"size": "0.64rem",
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "1.05rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.05rem, 2vw, 1.275rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.45rem, 3vw, 2rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(2.25rem, 4.5vw, 3.75rem)",
					"slug": "x-large"
				},
				{
					"name": "XXL",
					"size": "clamp(2.75rem, 6vw, 5.25rem)",
					"slug": "xx-large"
				}
			]
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1000px"
		}
	},
	"styles": {
		"blocks": {
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": 1.4
				}
			},
			"core/button": {
				"border": {
					"border": "1px",
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--dark-gray)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)",
					"textTransform": "uppercase"
				}
			},
			"core/code": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--unit)",
						"bottom": "var(--wp--custom--spacing--unit)",
						"left": "var(--wp--custom--spacing--unit)",
						"right": "var(--wp--custom--spacing--unit)"
					}
				},
				"border": {
					"width": "1.5px",
					"radius": "0px",
					"color": "var(--wp--preset--color--dark-gray)",
					"style": "solid"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--merriweather)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--page-title)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lato)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "var(--wp--custom--typography--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "var(--wp--custom--typography--font-weight--bold)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-slab)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-slab)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-slab)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto-slab)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	}
}
